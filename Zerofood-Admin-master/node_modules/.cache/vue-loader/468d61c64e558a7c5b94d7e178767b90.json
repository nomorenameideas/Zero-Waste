              >\n                        </v-col>\n                        <v-col>\n                          <v-btn\n                            color=\"white--text mx-2 primary accent-4\"\n                            @click=\"changeOrderStatus(item, 'accept')\"\n                            >Accept</v-btn\n                          >\n                          <v-btn\n                            color=\"white--text mx-2 success accent-4\"\n                            @click=\"changeOrderStatus(item, 'complete')\"\n                            >Complete</v-btn\n                          >\n                          <v-btn\n                            color=\"white--text mx-2 blue-grey\"\n                            @click=\"changeOrderStatus(item, 'cancle')\"\n                            >Cancle</v-btn\n                          >\n                        </v-col>\n                      </v-row>\n                    </tbody>\n                  </template>\n                </v-simple-table>\n              </v-list-tile-content>\n            </v-list-tile>\n          </v-list-group>\n        </v-list>\n      </v-card>\n    </v-flex>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'\nexport default {\n  data: () => ({\n    multiLine: true,\n    snackbar: false,\n    text: '',\n    color: '',\n    dialog: false,\n    search: '',\n    headers: ['Meal Name', 'Price', 'QTY'],\n    items: [],\n    orderStatus: {\n      null: 'Pendding',\n      1: 'Accepted',\n      2: 'Completed',\n      3: 'Refused'\n    }\n  }),\n\n  created() {\n    this.initialize()\n  },\n\n  methods: {\n    initialize() {\n      this.$axios\n        .get('/order')\n        .then(response => (this.items = response.data.orders))\n    },\n    changeOrderStatus({ order }, url) {\n      this.$axios.post(`/order/${url}/${order.id}`)\n    }\n  }\n}\n</script>\n"]}]}